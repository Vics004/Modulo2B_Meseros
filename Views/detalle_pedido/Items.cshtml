@{
}
@model List<item>

<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item">
            <input type="checkbox" name="seleccionados" value="@item.itemId" />
            @item.nombre - @item.precio
        </li>
    }
</ul>

